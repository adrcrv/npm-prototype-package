{
  "name": "npm-prototype-package",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "check": "pnpm lint && pnpm parse",
    "lint": "eslint .",
    "parse": "tsc -p tsconfig.json --noEmit",
    "build": "pnpm build:static && pnpm build:core && pnpm build:prepare",
    "build:core": "tsc -p tsconfig.json",
    "build:static": "copyfiles -u 1 -a ./src/**/*.md ./dist",
    "build:prepare": "cp package.json .npmrc ./dist",
    "publish": "pnpm publish ./dist",
    "postbuild": "./src/scripts/postinstall.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/compat": "1.1.0",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.5.0",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@typescript-eslint/parser": "7.13.1",
    "acorn": "8.12.0",
    "copyfiles": "2.4.1",
    "eslint": "8.56.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "typescript": "5.4.5",
    "typescript-eslint": "7.13.1"
  }
}
